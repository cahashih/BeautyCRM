<Window x:Class="LobovDiplom.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LobovDiplom"
        mc:Ignorable="d"
        Title="MainWindow" Height="500" Width="800">
    <Window.Resources>
        













    </Window.Resources>
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="1*"/>

        </Grid.RowDefinitions>
        <Border Background="#3FFC8198" Grid.RowSpan="2"></Border>
        <Image Source="/LogoName.png" />
        <Frame x:Name="MainFrame" Grid.Row="1" NavigationUIVisibility="Hidden" ></Frame>

        <Button Click="Button_Click" Width="400" Height="100" Foreground="{x:Null}" BorderBrush="{x:Null}" Background="#00DDDDE1" />


    </Grid>

</Window>
